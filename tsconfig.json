{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "module": "ES2022",
    "lib": [
      "es2020"
    ],
    "target": "es2019",
    "isolatedModules": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "checkJs": true,
    "baseUrl": "C:/Users/18115/Desktop/GKBlog",
    "paths": {
      "$components": [
        "./src/components"
      ],
      "$": [
        "./src"
      ]
    },
    "types": [
      "astro/env"
    ],
    "strictNullChecks": true,
    "jsx": "preserve"
  },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.js",
    "src/**/*.ts",
    "src/pages/rss.xml.js"
  ]
}